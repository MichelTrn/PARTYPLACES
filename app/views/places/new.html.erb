
<div class="container mt-5 border">
  <h1 class="text-center mt-5 ">Add your place</h1>
  <%= simple_form_for (@place) do |f| %>
    <%= f.input :name %>
    <%= f.input :address %>
    <%= f.input :price %>
    <%= f.input :photo, as: :file%>
    <%= f.button :submit, class: "btn btn-outline-primary"  %>
  <% end %>
</div>
