<div class="container mt-5">
  <h1 class="text-center border">Edit my place</h1>
  <%= simple_form_for (@place) do |f| %>
    <%= f.input :name %>
    <%= f.input :address %>
    <%= f.input :price %>
    <%= f.input :picture_url %>
    <%= f.button :submit, class: "btn btn-success"  %>
  <% end %>
</div>
